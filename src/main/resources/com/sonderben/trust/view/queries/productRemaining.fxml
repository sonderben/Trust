<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="main-bg" stylesheets="@../../css/general.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sonderben.trust.controller.queries.RemainingProductController">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0">
              <columns>
                <TableColumn fx:id="codeCol" prefWidth="75.0" text="%code" />
                <TableColumn fx:id="descriptionCol" prefWidth="75.0" text="%description" />
                  <TableColumn fx:id="categoryCol" prefWidth="75.0" text="%category" />
                  <TableColumn fx:id="qtyEnteredCol" prefWidth="75.0" text="%qty_entered" />
                  <TableColumn fx:id="qtyRemainingCol" prefWidth="75.0" text="%qty_remaining" />
                  <TableColumn fx:id="expirationCol" prefWidth="75.0" text="%exp_date" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>
