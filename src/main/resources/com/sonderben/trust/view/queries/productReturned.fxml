<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="main-bg" stylesheets="@../../css/general.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sonderben.trust.controller.queries.ProductReturned">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0">
              <columns>
                  <TableColumn fx:id="invoiceCol" prefWidth="75.0" text="%invoice" />
                  <TableColumn fx:id="dateBoughtCol" prefWidth="75.0" text="%date_bought" />
                  <TableColumn fx:id="dateReturnecol" prefWidth="75.0" text="%date_returned" />
                  <TableColumn fx:id="reasonCol" prefWidth="75.0" text="%reason" />
                  <TableColumn fx:id="actionCol" prefWidth="75.0" text="%action" />
                <TableColumn fx:id="codCol" prefWidth="75.0" text="%code" />
                <TableColumn fx:id="desciptionCol" prefWidth="75.0" text="%description" />
                  <TableColumn fx:id="quantityCol" prefWidth="75.0" text="%qty" />
                  <TableColumn fx:id="totalCol" prefWidth="75.0" text="%total" />
                  <TableColumn fx:id="dateExpiredCol" prefWidth="75.0" text="%exp_date" />
                  <TableColumn fx:id="employeeFullNameCol" prefWidth="75.0" text="%employee" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>
