                  <TextField fx:id="emailTextField" promptText="Email" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="accountNumberTextField" promptText="Account number" GridPane.columnIndex="2" GridPane.rowIndex="3" />
                  <TextField fx:id="telephoneTextField" promptText="Telephone" GridPane.columnIndex="3" GridPane.rowIndex="3" />
                  <TextField fx:id="userNameTextField" promptText="User name" GridPane.rowIndex="3" />
                  <TextField fx:id="scheduleTextField" editable="false" onMouseClicked="#scheduleOnMOuseClick" promptText="Schedules" GridPane.rowIndex="5" />
                  <DatePicker fx:id="birthdayDatePicker" promptText="Birtday" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                  <PasswordField fx:id="passwordField" promptText="Password" GridPane.columnIndex="2" GridPane.rowIndex="5" />
                  <ChoiceBox fx:id="choiceBoxGender" prefWidth="150.0" GridPane.columnIndex="3" GridPane.rowIndex="5">
                     <items>
                        <FXCollections fx:factory="observableArrayList">
                           <String fx:value="Male" />
                           <String fx:value="Female" />
                           <String fx:value="No binary" />
                        </FXCollections>
                     </items>
                  </ChoiceBox>
                  <TextField fx:id="directionField" promptText="Direction" GridPane.rowIndex="7" />
                  <TextField fx:id="passportTextField" promptText="Passport" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <ChoiceBox fx:id="choiceBoxRole" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="5" />
